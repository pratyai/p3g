sym N
var i, j
decl A
out A

; MAIN OUTER LOOP
(A[0,0] => A[0,0]) L_i | for i = 0 to N-1:
    (A[0,0] => A[0,0]) L_j_lt | for j = 0 to i-1:
        (A[i,j], A[i,0:j-1], A[0:j-1,j] => A[i,j]) S_sub_matmul | op(A_update_sub_matmul)
        (A[i,j], A[j,j] => A[i,j]) S_div | op(A_update_div)

    (A[0,0] => A[0,0]) L_j_ge | for j = i to N-1:
        (A[i,j], A[i,0:i-1], A[0:i-1,j] => A[i,j]) S_sub_matmul2 | op(A_update_sub_matmul_2)
