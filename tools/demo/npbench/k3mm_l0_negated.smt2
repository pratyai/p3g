(declare-fun NI () Int)
(declare-fun NJ () Int)
(declare-fun NK () Int)
(declare-fun i () Int)

(assert (and
  (forall ((j_0 Int) (j_1 Int) (k_0 Int) (k_1 Int))
    (or
      (not (<= 0 j_0))
      (not (<= j_0 (- NJ 1)))
      (not (<= 0 j_1))
      (not (<= j_1 (- NJ 1)))
      (not (= i (+ i 1)))
      (not (= j_0 j_1))
      (not (<= 0 k_1))
      (not (<= k_1 (- NK 1)))
      (not (<= 0 k_0))
      (not (<= k_0 (- NK 1)))
    )
  )
  (forall ((j_0 Int) (j_1 Int) (k_0 Int))
    (or
      (not (<= 0 j_0))
      (not (<= j_0 (- NJ 1)))
      (not (<= 0 j_1))
      (not (<= j_1 (- NJ 1)))
      (not (= i (+ i 1)))
      (not (= j_0 j_1))
      (not (<= 0 k_0))
      (not (<= k_0 (- NK 1)))
    )
  )
  (forall ((j_0 Int) (j_1 Int))
    (or
      (not (<= 0 j_0))
      (not (<= j_0 (- NJ 1)))
      (not (<= 0 j_1))
      (not (<= j_1 (- NJ 1)))
      (not (= i (+ i 1)))
      (not (= j_0 j_1))
    )
  )
  (forall ((j_0 Int) (j_1 Int) (k_1 Int))
    (or
      (not (<= 0 j_0))
      (not (<= j_0 (- NJ 1)))
      (not (<= 0 j_1))
      (not (<= j_1 (- NJ 1)))
      (not (= i (+ i 1)))
      (not (= j_0 j_1))
      (not (<= 0 k_1))
      (not (<= k_1 (- NK 1)))
    )
  )
  (and
    (<= 0 i)
    (<= (+ i 1) (- NI 1))
    (<= 1 (- NK 1))
    (<= 1 (- NJ 1))
    (<= 1 (- NI 1))
  )
))

(check-sat)